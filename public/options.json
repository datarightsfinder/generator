    "dataProtectionRegister": {
      "helperPosition": "above",
      "label": "<span class=\"number\">5</span>Is the organisation in a data protection register?",
      "helper": "Find the details of this organisation in a national data processors register, like the <a href=\"https://ico.org.uk/about-the-ico/what-we-do/register-of-fee-payers/\" target=\"_blank\">ICO register of fee payers</a>. Some countries may not have one.",
      "fields": {
        "present": {
          "type": "radio",
          "optionLabels": [
            "Yes", "No"
          ],
          "sort": false,
          "hideNone": true
        }
      }
    },
    "privacyShield": {
      "helperPosition": "above",
      "helper": "The <a href=\"https://www.privacyshield.gov/welcome\" target=\")blank\">Privacy Shield website</a> has a searchable list of organisations in this scheme.",
