    "dataProtectionOfficer": {
      "helperPosition": "above",
      "label": "<span class=\"number\">4</span>Do they have a Data Protection Officer?",
      "helper": "",
      "fields": {
        "present": {
          "type": "radio",
          "optionLabels": [
            "Yes", "No"
          ],
          "sort": false,
          "hideNone": true
        }
